<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape>
            <solid android:color="#0000" />
        </shape>
    </item>

    <item android:bottom="12dp">
        <shape android:shape="rectangle" >
